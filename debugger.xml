<application>
  <component name="XDebuggerSettings">
    <data-views>
      <option name="showLibraryStackFrames" value="false" />
    </data-views>
    <general />
    <debuggers>
      <debugger id="javascript">
        <configuration>
          <custom-object-presentation enabled="true">
            <properties>
              <property value="contents" />
              <property value="id" />
              <property value="length" />
              <property value="name" />
              <property value="type" />
              <property value="value" />
            </properties>
          </custom-object-presentation>
          <stepping-filters-enabled>false</stepping-filters-enabled>
        </configuration>
      </debugger>
      <debugger id="ruby">
        <configuration>
          <option name="verboseOutput" value="true" />
        </configuration>
      </debugger>
    </debuggers>
  </component>
</application>